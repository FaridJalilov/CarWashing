class Barn
{
	float water;
	int powder_packs;
	int chemic_powder_packs;
public:
	Barn() = default;
	Barn(float water, int powder_packs,int chemic_powder_packs)
	{
		this->water = water;
		this->powder_packs = powder_packs;
		this->chemic_powder_packs = chemic_powder_packs;
	}

	void SetWater(float water)
	{
		this->water = water;
	}
	float GetWater()
	{
		return water;
	}

	void SetPowder(int powder)
	{
		this->powder_packs = powder;
	}
	float GetPowder()
	{
		return powder_packs;
	}	
	
	void SetChePowder(int che_powder)
	{
		this->chemic_powder_packs = che_powder;
	}
	float GetChePowder()
	{
		return chemic_powder_packs;
	}
};
